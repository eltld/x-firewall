<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:background="@color/white"
        >
    <LinearLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dip"
            android:layout_marginBottom="5dip"
            >
        <TextView android:layout_height="wrap_content" android:id="@+id/block_log_item_text1"
                  android:paddingLeft="30dp"
                  android:layout_width="wrap_content"  android:textSize="18sp"
                  android:textAppearance="?android:attr/textAppearanceMedium"
                  android:textColor="@color/table_text_selector" />
                />
        <TextView android:layout_height="wrap_content" android:id="@+id/block_log_item_text2"
                  android:paddingLeft="30dp"
                  android:layout_width="wrap_content" android:textSize="12sp"
                  android:textAppearance="?android:attr/textAppearanceMedium"
                  android:textColor="@color/table_text_selector" />
                />

    </LinearLayout>



</LinearLayout>
